{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "x-forwarded-host": "www.go-aei.com"
      },
      "continue": true
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "https://go-aei.com/:path*",
      "destination": "https://www.go-aei.com/:path*",
      "permanent": true
    }
  ],
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true,
    "auto": true
  },
  "git": {
    "deploymentEnabled": true
  }
} 