{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-forwarded-host",
          "value": "www.go-aei.com"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "go-aei.com"
        }
      ],
      "destination": "https://www.go-aei.com/:path*",
      "permanent": true
    }
  ],
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true,
    "auto": true,
    "productionBranch": "main"
  },
  "git": {
    "deploymentEnabled": true
  },
  "alias": ["www.go-aei.com", "dev.go-aei.com"]
} 